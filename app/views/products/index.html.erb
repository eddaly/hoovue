<h1>Listing products</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Genre</th>
    <th>User</th>
    <th>Image</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @products.each do |product| %>
  <tr>
  	   <td><%= product.id %></td>
    <td><%= product.title %></td>
  
    
  </tr>
<% end %>
</table>

<br />

<h2>Import Products</h2>

<%= form_tag import_products_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>

<%= link_to 'New Product', new_product_path %>
